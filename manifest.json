{
  "manifest_version": 2,

  "name": "Swift Snake 3D",
  "short_name": "Swift Snake",
  "description": "Lets you play snake in 3D and in hyper speed whenever you want...",
  "version": "2.379",
  "key": "",
  "content_security_policy": "script-src 'self' https://apis.google.com; object-src 'self'",

  "icons":{
    "16":"boo_icon_16.png",
    "128":"boo_icon_128.png"
  },

  "browser_action": {
    "default_icon": "boo_icon_128.png",
    "default_popup": "index.html"
  },
  "background": {
    "scripts": [
      "js/jquery.js"
    ]
  },

  "permissions": [
    "activeTab",
    "tabs",
    "storage",
    "identity",
    "https://ajax.googleapis.com/",
    "https://apis.google.com/"
  ],
  "oauth2": {
    "client_id":"491250167307-58o5k8lnbal68vd1qlcbk64l5biqsjb8.apps.googleusercontent.com",
    "scopes":[
      "https://www.googleapis.com/auth/userinfo.email",
      "https://www.googleapis.com/auth/userinfo.profile"
    ]
  }
}
